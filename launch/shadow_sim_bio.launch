<?xml version="1.0"?>
<launch>
    <node name="bioik_dataset" pkg="shadow_regression" type="bioik_dataset" output="screen">
        <param name="mapfile" value="$(find shadow_regression)/data/training/human_robot_mapdata_pip2.csv"/>
        <param name="depth_img_path" value="$(find shadow_regression)/data/depth_shadow/"/>
        <param name="rgb_img_path" value="$(find shadow_regression)/data/test/"/>
        <param name="handshape" value="$(find shadow_regression)/data/handshape/"/>
        <param name="jointsfile" value="$(find shadow_regression)/data/training/joints_file.csv"/>
        <param name="end_effectorfile" value="$(find shadow_regression)/data/training/end_effector_file.csv"/>

    </node>
</launch>
